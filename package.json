{
  "name": "speckle-email-templates",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "env:up": "docker-compose up -d",
    "env:down": "docker-compose down",
    "build": "mjml ./src/*.mjml -o ./dist/",
    "watch": "mjml -w ./src/*.mjml -o ./dist/"
  },
  "devDependencies": {
    "mjml": "^4.13.0",
    "prettier": "^2.7.1"
  }
}
